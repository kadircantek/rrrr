# Firebase Configuration (Production)
# Get these values from Firebase Console > Project Settings
VITE_FIREBASE_API_KEY=your-production-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_DATABASE_URL=https://your-project.firebaseio.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=your-app-id
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Payment Provider (Choose one)
# Paddle
VITE_PADDLE_VENDOR_ID=your-paddle-vendor-id
VITE_PADDLE_PRODUCT_ID_PRO=your-pro-product-id
VITE_PADDLE_PRODUCT_ID_UNLIMITED=your-unlimited-product-id

# LemonSqueezy
VITE_LEMONSQUEEZY_STORE_ID=your-store-id
VITE_LEMONSQUEEZY_VARIANT_ID_PRO=your-pro-variant-id
VITE_LEMONSQUEEZY_VARIANT_ID_UNLIMITED=your-unlimited-variant-id

# Environment
VITE_APP_ENV=production
VITE_APP_VERSION=1.0.0
